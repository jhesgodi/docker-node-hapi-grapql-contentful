{
  "name": "NaaS",
  "version": "1.0.0",
  "description": "Hapi + GraphQL",
  "author": "JhesG",
  "main": "server.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "eslint": "eslint {src,tests}/**",
    "lint": "npm run eslint",
    "start:server": "babel-node server.js --exec babel-node --presets es2015,stage-2",
    "start": "nodemon server.js --exec babel-node --presets es2015,stage-2",
    "test:contracts": "babel-node node_modules/.bin/consumer-contracts run",
    "test:watch": "npm run test -- -w",
    "test": "mocha tests/**/*.spec.js --compilers js:babel-core/register"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "eslint-config-airbnb": "10.0.1",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.2.1",
    "eslint-plugin-react": "6.2.0",
    "eslint": "3.4.0",
    "mocha": "3.0.2",
    "nodemon": "^1.10.2"
  },
  "dependencies": {
    "bcrypt": "~0.8.7",
    "consumer-contracts": "1.4.0",
    "contentful": "3.5.0",
    "expect": "1.20.2",
    "good": "~7.0.1",
    "good-console": "~6.1.2",
    "good-squeeze": "~4.0.0",
    "graphql": "0.6.0",
    "handlebars": "~4.0.5",
    "handlebars-helpers": "0.7.5",
    "hapi": "^15.0.3",
    "hapi-auth-basic": "~4.2.0",
    "hapi-auth-cookie": "~6.1.1",
    "hapi-graphql": "1.0.1",
    "inert": "4.0.2",
    "lodash": "4.15.0",
    "node-fetch": "1.6.0",
    "vision": "~4.1.0"
  }
}
